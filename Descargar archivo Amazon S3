# Instalar las bibliotecas necesarias
!pip install boto3 pandas

# Configurar las credenciales de AWS
import os
os.environ['AWS_ACCESS_KEY_ID'] = 'TU_ACCESS_KEY_ID'
os.environ['AWS_SECRET_ACCESS_KEY'] = 'TU_SECRET_ACCESS_KEY'

# Descargar el archivo de S3
import boto3
s3 = boto3.client('s3')
bucket_name = 'TU_BUCKET_NAME'
s3_file_name = 'df_final_SMOTE_ENN.csv'
local_file_name = 'df_final_SMOTE_ENN.csv'
s3.download_file(bucket_name, s3_file_name, local_file_name)
print(f"Archivo {s3_file_name} descargado como {local_file_name}")

# Leer el archivo CSV con pandas
import pandas as pd
df = pd.read_csv(local_file_name)
print(df.head())
